{
  "name": "watney",
  "version": "0.0.1",
  "description": "Mark Watney ASCII Simulator",
  "keywords": [
    "mark watney",
    "the martian"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc",
    "test": "node --import tsx --test **/**/*.test.tsx **/**/*.test.ts",
    "check": "npm run lint && npm run typecheck && npm run test",
    "start": "parcel ./app/index.html",
    "build": "parcel build ./app/index.html",
    "clean": "rm -rf .parcel-cache dist",
    "postinstall": "jq '.exports.\"./build/spyMatchers\"=\"./build/spyMatchers.js\"' node_modules/expect/package.json | sponge node_modules/expect/package.json"
  },
  "imports": {
    "#comps/*": "./src/components/*.tsx",
    "#logic/*": "./src/logic/*.ts",
    "#test": "./test/tools/test.ts",
    "#render": "./test/tools/render.ts"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "expect": "^29.7.0",
    "global-jsdom": "^25.0.0",
    "globals": "^15.9.0",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  }
}
